# Template file for 'winetricks-git'
_my_field_repo=git://github.com/Winetricks/winetricks.git
archs=noarch
build_style=gnu-makefile
conflicts=winetricks
depends="cabextract unzip xmessage wine"
homepage="http://wiki.winehq.org/winetricks"
hostmakedepends="git"
license="LGPL-3.0-or-later"
maintainer="Orphaned <orphan@voidlinux.org>"
pkgname=winetricks-git
provides=winetricks-0_1
revision=1
short_desc="Script to install various redistributable runtime libraries in Wine"
version=dc9a7c2

winetricks-git_package() {
    true
}

do_fetch() {
    git clone $_my_field_repo ${pkgname}-${version}
}
