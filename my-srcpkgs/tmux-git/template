# Template file for 'tmux-git'
_my_field_repo=git://github.com/tmux/tmux
_my_field_skip_version=7922f4e
build_style=gnu-configure
conflicts=tmux
homepage="https://tmux.github.io"
hostmakedepends="
    autoconf
    automake
    bison
    git
    pkg-config
"
license="ISC"
maintainer="Orphaned <orphan@voidlinux.org>"
makedepends="
    libevent-devel
    ncurses-devel
"
pkgname=tmux-git
provides=tmux-0_1
revision=1
short_desc="Terminal Multiplexer"
version=37919a6

tmux-git_package() {
    true
}

do_fetch() {
    git clone $_my_field_repo ${pkgname}-${version}
}

pre_configure() {
    ./autogen.sh
}

post_install() {
    vlicense COPYING
}
